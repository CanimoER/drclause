# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Supabase MCP Server
SUPABASE_ACCESS_TOKEN=your_supabase_access_token

# OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_OPENAI_MODEL=gpt-4o

# Anthropic Configuration (Alternative LLM)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key
VITE_ANTHROPIC_MODEL=claude-3-sonnet-20240229

# Application Configuration
VITE_APP_NAME=drclause
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=development

# File Upload Limits
VITE_MAX_FILE_SIZE=10485760  # 10MB in bytes
VITE_ALLOWED_FILE_TYPES=.pdf,.docx,.xlsx,.txt

# Rate Limiting
VITE_RATE_LIMIT_REQUESTS=3
VITE_RATE_LIMIT_WINDOW=60000  # 1 minute in milliseconds

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true

# Internationalization
VITE_DEFAULT_LOCALE=en
VITE_FALLBACK_LOCALE=en 